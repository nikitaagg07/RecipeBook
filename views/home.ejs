<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Recipe Book</title>
    <style>
        /* Inline CSS for styling */
        body {
            font-family: Arial, sans-serif;
            margin: 0;
            padding: 0;
            display: flex;
            justify-content: center;
            align-items: center;
            background-color: #f4f4f4;
        }

        .container {
            width: 80%;
            margin: 20px auto;
        }

        header {
            background-image: url('data:image/jpeg;base64,');
            background-size: cover;
            color: white;
            text-align: center;
            padding: 40px;
        }

        header h1 {
            font-size: 2.5em;
            margin: 0;
        }

        .content {
            display: flex;
            gap: 20px;
            margin-top: 20px;
        }

        .left,
        .right {
            width: 50%;
            padding: 20px;
            background-color: #fff;
            border-radius: 8px;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        }

        form,
        .recipe-list {
            margin-top: 20px;
        }

        .recipe-list div {
            padding: 10px;
            border-bottom: 1px solid #ddd;
        }

        .recipe-list div a {
            margin-right: 10px;
            text-decoration: none;
            color: #007bff;
        }

        .recipe-list div a:hover {
            text-decoration: underline;
        }
    </style>


</head>
<body>

    <div class="container">
        <header>
            <h1>Recipe Book</h1>
        </header>

        <div class="content">
            <!-- Left side: Add Recipe Form -->
            <div class="left">
                <h2>Add Recipe</h2>
                <form action="/api/recipes" method="POST">
                    <label for="name">Recipe Name:</label><br>
                    <input type="text" id="name" name="name" required><br><br>

                    <label for="ingredients">Ingredients:</label><br>
                    <textarea id="ingredients" name="ingredients" required></textarea><br><br>

                    <label for="method">Method of Preparation:</label><br>
                    <textarea id="method" name="method" required></textarea><br><br>

                    <button type="submit">Submit</button>
                </form>
            </div>

            <!-- Right side: Search and Recipe List -->
            <div class="right">
                <h2>Search Recipes</h2>
                <form action="/search" method="GET">
                    <input type="text" name="q" placeholder="Search for a recipe" required>
                    <button type="submit">Search</button>
                </form>

                <h3>Available Recipes</h3>
                <div class="recipe-list">
                    <% if (recipes && recipes.length> 0) { %>
                        <% recipes.forEach(recipe=> { %>
                            <div>
                                <a href="/recipe/<%= recipe._id %>">
                                    <%= recipe.name %>
                                </a>
                                <a href="/recipe/<%= recipe._id %>">View</a> |
                                <a href="#"
                                    onclick="return confirmDelete('<%= recipe._id %>')">Delete</a>
                            </div>
                            <% }) %>
                                <% } else { %>
                                    <p>No recipes found.</p>
                                    <% } %>
                </div>
            </div>
        </div>
    </div>

    <script>
        function confirmDelete(id) {
            if (confirm('Are you sure you want to delete this recipe?')) {
                fetch(`/api/recipes/${id}`, { method: 'DELETE' })
                    .then(response => {
                        if (response.ok) {
                            alert('Recipe deleted successfully');
                            window.location.reload(); // Reload the page to reflect the changes
                        } else {
                            alert('Error deleting recipe');
                        }
                    })
                    .catch(error => console.error('Error:', error));
            }
            return false; // Prevent default link action
        }
    </script>
    
</body>

</html>